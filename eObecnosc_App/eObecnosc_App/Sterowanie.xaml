<UserControl x:Class="eObecnosc_App.Sterowanie"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:eObecnosc_App"
             mc:Ignorable="d" 
             d:DesignHeight="640" d:DesignWidth="1274" Background="White">
    <Grid>
        <StackPanel x:Name="Panel" HorizontalAlignment="Left" Height="640" Margin="0,50,-0.4,0" VerticalAlignment="Top" Width="1274"/>
        <Button x:Name="Przycisk_Wyswietl" Content="Obecność" HorizontalAlignment="Left" VerticalAlignment="Top" Width="120" Height="50" Click="Przycisk_Wyswietl_Click" />
        <Button x:Name="Przycisk_Dodaj" Content="Dodaj Zajęcia" HorizontalAlignment="Left" Margin="125,0,0,0" VerticalAlignment="Top" Width="120" Height="50" Click="Przycisk_Dodaj_Click" />
        <Button x:Name="Przycisk_Edytuj" Content="Edytuj Zajęcia" HorizontalAlignment="Left" Margin="250,0,0,0" VerticalAlignment="Top" Width="120" Height="50" Click="Przycisk_Edytuj_Click" />

    </Grid>
</UserControl>
