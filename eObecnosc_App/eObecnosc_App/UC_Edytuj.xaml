<UserControl x:Class="eObecnosc_App.UC_Edytuj"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:eObecnosc_App"
             mc:Ignorable="d" 
             d:DesignHeight="640" d:DesignWidth="1274" Background="White">
    <Grid>
        <Label x:Name="Label_Edytuj" Content="Opcje:" HorizontalAlignment="Left" Margin="80,41,0,0" VerticalAlignment="Top"/>
        <Button x:Name="Dodaj_przedmiot" Content="Dodaj przedmiot" HorizontalAlignment="Left" Margin="80,93,0,0" VerticalAlignment="Top" Width="117" Click="Dodaj_przedmiot_Click"/>
        <Button x:Name="Dodaj_grupe" Content="Dodaj grupę" HorizontalAlignment="Left" Margin="80,146,0,0" VerticalAlignment="Top" Width="117"/>
        <Button x:Name="Import" Content="Importuj" HorizontalAlignment="Left" Margin="80,200,0,0" VerticalAlignment="Top" Width="117"/>
        <Button x:Name="Eksport" Content="Eksportuj" HorizontalAlignment="Left" Margin="80,249,0,0" VerticalAlignment="Top" Width="117"/>
        <Grid x:Name="Edycja_Grid" HorizontalAlignment="Left" Height="500" Margin="292,93,0,0" VerticalAlignment="Top" Width="900"/>
    </Grid>
</UserControl>
