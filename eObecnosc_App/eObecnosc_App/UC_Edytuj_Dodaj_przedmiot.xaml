<UserControl x:Class="eObecnosc_App.UC_Edytuj_Dodaj_przedmiot"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:eObecnosc_App"
             mc:Ignorable="d" 
             d:DesignHeight="500" d:DesignWidth="900" Background="White">
    <Grid>
        <Label x:Name="UC_Edytuj_Dodaj_przedmiot_Label" Content="Podaj nazwę przdmiotu:" HorizontalAlignment="Left" Margin="229,80,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="UC_Edytuj_Dodaj_przedmiot_Nazwa_przedmiotu" HorizontalAlignment="Left" Height="23" Margin="229,134,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="136"/>
        <Button x:Name="UC_Edytuj_Dodaj_przedmiot_Przycisk_dodaj" Content="Dodaj" HorizontalAlignment="Left" Margin="290,234,0,0" VerticalAlignment="Top" Width="75" Click="UC_Edytuj_Dodaj_przedmiot_Przycisk_dodaj_Click"/>

    </Grid>
</UserControl>
